{
  "name": "gap-map",
  "type": "module",
  "version": "1.0",
  "scripts": {
    "dev": "node --import tsx scripts/prebuild.ts && astro dev",
    "dev:full": "node --import tsx scripts/prebuild.ts --full-refresh && astro dev",
    "start": "astro dev",
    "prebuild": "node --import tsx scripts/prebuild.ts",
    "build": "node --import tsx scripts/prebuild.ts && node --import tsx scripts/createDataZip.js && astro build",
    "build:full": "node --import tsx scripts/prebuild.ts --full-refresh && node --import tsx scripts/createDataZip.js && astro build",
    "build:clean": "node --import tsx scripts/prebuild.ts --clear-cache --full-refresh && node --import tsx scripts/createDataZip.js && astro build",
    "preview": "astro preview",
    "test": "vitest run",
    "functions:dev": "netlify-lambda serve netlify/functions",
    "cache:clear": "node --import tsx scripts/prebuild.ts --clear-cache",
    "export-data": "node --import tsx scripts/dataExport.js",
    "create-zip": "node --import tsx scripts/createDataZip.js",
    "export-data:debug": "node --import tsx scripts/dataExport.js > export-log.txt 2>&1",
    "create-zip:debug": "node --import tsx scripts/createDataZip.js > zip-log.txt 2>&1"
  },
  "dependencies": {
    "@astrojs/react": "latest",
    "@notionhq/client": "latest",
    "@pixi/events": "^7.3.2",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "archiver": "^5.3.2",
    "astro": "latest",
    "d3": "^7.9.0",
    "d3-force-3d": "^3.0.5",
    "dotenv": "latest",
    "fuse.js": "^7.1.0",
    "marked": "^15.0.8",
    "nanostores": "^0.11.4",
    "netlify-lambda": "latest",
    "notion-to-md": "^3.1.1",
    "pixi-viewport": "^5.0.1",
    "pixi.js": "^7.3.2",
    "react": "latest",
    "react-dom": "latest",
    "rehype-raw": "^6.1.1",
    "rehype-stringify": "^9.0.4",
    "remark": "^14.0.3",
    "remark-parse": "^10.0.2",
    "remark-rehype": "^10.1.0",
    "sass": "latest"
  },
  "devDependencies": {
    "@types/archiver": "^5.3.2",
    "@types/node": "^22.14.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "vitest": "latest"
  }
}
